{
	"version": "2.0.0",
	"tasks": [
        {
            "type": "cargo",
            "command": "build",
            "problemMatcher": [
                "$rustc",
                "$rust-panic"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "label": "rust: cargo build"
        },
        {
            "type": "cargo",
            "command": "test",
            "problemMatcher": [
                "$rustc",
                "$rust-panic"
            ],
            "group": {
                "kind": "test",
                "isDefault": false
            },
            "label": "rust: cargo test"
        },
        {
            "type": "cargo",
            "command": "run",
            "problemMatcher": [
                "$rustc",
                "$rust-panic"
            ],
            "group": {
                "kind": "run",
                "isDefault": false
            },
            "label": "rust: cargo run"
        }
    ]
}
